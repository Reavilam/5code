@page
@model AsignaSalones.App.Frontend.Pages.EditarContagiadoModel
@{
}
<h1>Editar Contagiados</h1>
<form method="post">
    <input type="hidden" asp-for="contagiado.id">
    <div class="form-group">
        <label asp-for="contagiado.nombre " >contagiado</label>
        <input asp-for="contagiado.nombre" class="form-control">
        <span class="text_danger" asp-validation-for="contagiado.nombre"></span>
    </div>
    <div class="form-group">
        <label asp-for="contagiado.apellidos"></label>
        <input asp-for="contagiado.apellidos" class="form-control">
        <span class="text_danger" asp-validation-for="contagiado.apellidos"></span>
    </div>    

    <div class="form-group">
        <label asp-for="contagiado.fechaContagio"></label>
        <input asp-for="contagiado.fechaContagio" class="form-control">
    </div>

    <div class="form-group">
        <label asp-for="contagiado.sintomas"></label>
        <input asp-for="contagiado.sintomas" class="form-control">
        <span class="text_danger" asp-validation-for="contagiado.sintomas"></span>
    </div>    
    <div class="form-group">
        <label asp-for="contagiado.periodoAislamiento"></label>
        <input asp-for="contagiado.periodoAislamiento" class="form-control">
    </div>
    <button type="submit" class="btn btn-primary">Guardar Cambios</button>
</form>
